<h1>Sign Up!</h1>

<%= form_for(@user, url: signup_path) do |f|%>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <%= f.submit "Sign Up!" %>
<% end %>